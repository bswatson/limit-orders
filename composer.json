{
    "name": "nexcess/woocommerce-limit-orders",
    "description": "Automatically disable WooCommerce's checkout process after reaching a maximum number of orders.",
    "type": "wordpress-plugin",
    "license": "MIT",
    "keywords": [
        "woocommerce",
        "woocommerce-extension"
    ],
    "support": {
        "issues": "https://github.com/nexcess/woocommerce-limit-orders/issues",
        "source": "https://github.com/nexcess/woocommerce-limit-orders"
    },
    "readme": "README.md",
    "authors": [
        {
            "name": "Nexcess",
            "homepage": "https://www.nexcess.net"
        },
        {
            "name": "Steve Grunwell",
            "homepage": "https://stevegrunwell.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "assertwell/wp-core-test-framework": "^0.1.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.6",
        "phpcompatibility/phpcompatibility-wp": "*",
        "phpunit/phpunit": "^7.5",
        "wp-coding-standards/wpcs": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Nexcess\\WooCommerceLimitOrders\\": ["src/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": ["tests/"]
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "preferred-install": "dist",
        "sort-packages": true
    }
}
